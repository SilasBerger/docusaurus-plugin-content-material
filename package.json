{
  "name": "plugin-content-material",
  "version": "3.0.0",
  "description": "Docs plugin for Docusaurus.",
  "main": "lib/index.js",
  "sideEffects": false,
  "exports": {
    "./lib/*": "./lib/*",
    "./src/*": "./src/*",
    "./client": {
      "type": "./lib/client/index.d.ts",
      "default": "./lib/client/index.js"
    },
    "./server": {
      "type": "./lib/server-export.d.ts",
      "default": "./lib/server-export.js"
    },
    ".": {
      "types": "./src/plugin-content-material.d.ts",
      "default": "./lib/index.js"
    }
  },
  "types": "src/plugin-content-material.d.ts",
  "scripts": {
    "build": "tsc --build",
    "watch": "tsc --build --watch"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SilasBerger/docusaurus-plugin-content-material.git"
  },
  "license": "MIT",
  "dependencies": {
    "@docusaurus/core": "^3.0.1",
    "@docusaurus/logger": "3.0.0",
    "@docusaurus/mdx-loader": "3.0.0",
    "@docusaurus/module-type-aliases": "3.0.0",
    "@docusaurus/types": "3.0.0",
    "@docusaurus/utils": "3.0.0",
    "@docusaurus/utils-validation": "3.0.0",
    "@types/react-router-config": "^5.0.7",
    "@types/shelljs": "^0.8.15",
    "combine-promises": "^1.1.0",
    "fs-extra": "^11.1.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "tslib": "^2.6.0",
    "utility-types": "^3.10.0",
    "webpack": "^5.88.1"
  },
  "devDependencies": {
    "@swc/jest": "^0.2.29",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.11",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.202",
    "@types/picomatch": "^2.3.0",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "commander": "^5.1.0",
    "eslint-plugin-jest": "^27.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-serializer-ansi-escapes": "^2.0.1",
    "jest-serializer-react-helmet-async": "^1.0.21",
    "picomatch": "^2.3.1",
    "shelljs": "^0.8.5",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "engines": {
    "node": ">=18.0"
  }
}
